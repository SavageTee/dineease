    <div class="tw-py-8 tw-px-4 tw-mx-auto tw-w-full lg:tw-py-16 lg:px-6">
        <div class="alert alert-danger tw-mx-auto tw-block tw-text-center" role="alert">
            <i class="fa-solid fa-3x tw-text-red-700 fa-circle-exclamation"></i>
            <div class="col tw-font-extrabold tw-py-2" ><%= errorHeader %></div>
            <div id="error_body" class="col tw-font-medium tw-py-1"><%= errorBody %></div>
        </div>
    </div>
    
    <div class="tw-w-full tw-flex tw-justify-center tw-mx-auto">
        <button id="copy_error" type="button" class="btn btn-primary tw-border-r- mx-1 tw-border-none tw-rounded-none rounded-1"> <%= copyError %> <i class="fa-solid fa-copy"></i></button>
        <button id="go_back" type="button" class="btn btn-danger mx-1 tw-border-none tw-rounded-none rounded-1"> <%= goBack %> <i class="fa-solid fa-circle-xmark"></i></button>
    </div> 

    <br/><br/><br/><br/>
    <% if (showErrorScript) { %>
        <script type="text/javascript">
            setTimeout(function(){window.location.href = '/en/language?id=<%= companyUUID %>'}, 5000);
        </script>
    <% } %>
    <script  src="/js/bootstrap.min.js"> </script>
    <script  src="/js/jquery-3.7.1.min.js"> </script>
